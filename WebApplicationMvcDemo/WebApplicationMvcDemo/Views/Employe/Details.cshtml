@model WebApplicationMvcDemo.Models.Employee

@{
    ViewBag.Title = "Employee Details";
}

<h2>Employee Details</h2>

<table style="font-family:Arial">
    <tr>
        <td>
            <b>Employee Id:</b>
        </td>
        <td>
               @Model.Id
        </td>
    </tr>
    <tr>
        <td>
            <b>Employee Name:</b>
        </td>
        <td> @Model.Name</td>
    </tr>
    <tr>
        <td>
            <b>Employee Gender:</b>
        </td>
        <td>@Model.Gender</td>
    </tr>
    <tr>
        <td>
            <b>Employee City:</b>
        </td>
        <td>@Model.City</td>
    </tr>
</table>
